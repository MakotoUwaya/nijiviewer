{
  "name": "nextra-docs",
  "version": "0.0.1",
  "description": "OiChan Docs, Powered by Nextra",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001 --turbopack",
    "build": "next build",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start",
    "lint": "biome lint ./ --files-ignore-unknown=true",
    "lint:fix": "biome check --apply ./ --files-ignore-unknown=true",
    "format": "biome format ./ --write"
  },
  "dependencies": {
    "@next/third-parties": "15.2.1",
    "@tailwindcss/postcss": "^4.0.6",
    "next": "15.2.1",
    "nextra": "4.2.16",
    "nextra-theme-docs": "4.2.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.0.6",
    "ui": "*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "pagefind": "1.3.0",
    "tsconfig": "*",
    "typescript": "^5.0.0"
  }
}
