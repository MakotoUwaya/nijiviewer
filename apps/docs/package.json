{
  "name": "nextra-docs",
  "version": "0.0.1",
  "description": "OiChan Docs, Powered by Nextra",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001 --turbopack",
    "build": "next build",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start",
    "lint": "biome lint ./ --files-ignore-unknown=true",
    "lint:fix": "biome check --apply ./ --files-ignore-unknown=true",
    "format": "biome format ./ --write"
  },
  "dependencies": {
    "@microsoft/clarity": "1.0.0",
    "@next/third-parties": "15.2.4",
    "@tailwindcss/postcss": "4.1.0",
    "next": "15.2.4",
    "nextra": "4.2.17",
    "nextra-theme-docs": "4.2.17",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "4.1.0",
    "ui": "*"
  },
  "devDependencies": {
    "@types/node": "22.13.17",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "pagefind": "1.3.0",
    "tsconfig": "*",
    "typescript": "5.8.2"
  }
}
